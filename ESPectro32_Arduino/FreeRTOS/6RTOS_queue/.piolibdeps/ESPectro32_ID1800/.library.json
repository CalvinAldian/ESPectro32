{
    "description": "Library for ESPectro32", 
    "repository": {
        "url": "https://github.com/dycodex/ESPectro32.git", 
        "type": "git"
    }, 
    "platforms": [
        "espressif32"
    ], 
    "frameworks": [
        "arduino"
    ], 
    "version": "0.3.0", 
    "build": {
        "srcFilter": [
            "+<*>", 
            "+<../lib/**/*>", 
            "-<../lib/Adafruit*>"
        ], 
        "flags": [
            "-I lib/Commons", 
            "-I lib/WS281x", 
            "-I lib/Network", 
            "-I lib/FreeRTOS-Wrapper"
        ]
    }, 
    "dependencies": [
        {
            "name": "Adafruit IS31FL3731 Library"
        }, 
        {
            "name": "Adafruit GFX Library"
        }
    ], 
    "authors": [
        {
            "url": null, 
            "maintainer": false, 
            "email": "an.dri@me.com", 
            "name": "Andri Yadi"
        }, 
        {
            "maintainer": true, 
            "name": "Alwin Arrasyid", 
            "url": null, 
            "email": "alwin.ridd@gmail.com"
        }
    ], 
    "keywords": [
        "makestro", 
        "espectro32"
    ], 
    "id": 1800, 
    "name": "ESPectro32"
}